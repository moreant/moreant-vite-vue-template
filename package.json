{
  "name": "devify",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "tsc": "vue-tsc --noEmit",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
